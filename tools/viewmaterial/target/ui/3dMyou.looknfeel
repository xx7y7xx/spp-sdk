	<WidgetLook name="3dMyou/Titlebar">
		<PropertyDefinition name="CaptionColour" initialValue="FFFFFFFF" redrawOnWrite="true"/>
		<ImagerySection name="title_main">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"></UnifiedDim>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"></UnifiedDim>
					</Dim>
				</Area>
				<Image image="titlebarleft" imageset="3dMyou"></Image>	
				
			</ImageryComponent>
			
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<ImageDim image="titlebarleft" dimension="Width" imageset="3dMyou"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim type="RightEdge" scale="1">
							<DimOperator op="Subtract">
								<ImageDim image="titlebarright" dimension="Width" imageset="3dMyou"/>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"></UnifiedDim>
					</Dim>
				</Area>
				<Image image="titlebarmiddle" imageset="3dMyou"></Image>
					<HorzFormat type="Tiled" />
			</ImageryComponent>
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1">							
						</UnifiedDim>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"></UnifiedDim>
					</Dim>
				</Area>
				<Image image="titlebarright" imageset="3dMyou"></Image>				
				<HorzFormat type="RightAligned"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="title_text">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="10"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width"  scale="1"></UnifiedDim>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"></UnifiedDim>
					</Dim>
				</Area>
				<Colour colour="FFE7E7E7"/>
				<VertFormat type="CentreAligned"/>
			</TextComponent>
		</ImagerySection>
		<StateImagery name="Active">
			<Layer>
				<Section section="title_main"/>
				<Section section="title_text"/>
			</Layer>
		</StateImagery>
		<StateImagery name="Inactive">
			<Layer>
				<Section section="title_main"/>
				<Section section="title_text"/>
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="title_main">
					<Colours topRight="FFF7F7F7" topLeft="FFF7F7F7" bottomLeft="FFF7F7F7" bottomRight="FFF7F7F7"/>
				</Section>
				<Section section="title_text">
					<Colours topRight="FFF7F7F7" topLeft="FFF7F7F7" bottomLeft="FFF7F7F7" bottomRight="FFF7F7F7"/>
				</Section>
			</Layer>
		</StateImagery>
	</WidgetLook>
